<MainAppScreen>:
	BoxLayout:
		#orientation : 'vertical'
		height: "80dp"
		spacing:10
		padding:10
		background_color: 0,1,1,1
		canvas:
			Rectangle:	
				pos:self.pos
				size:self.size
				source:'/home/anuja/Desktop/11.jpg'	
		Label:
			text: 'Mental Health Care'
			color: 1,1,0,1
			size_hint: 0.4,0.15
			font_size:'80sp'
			pos_hint: {'center_x':.85, 'center_y': .55}		
	BoxLayout:
		Button:
			size_hint:1,0.2
			text: 'Mood'
			font_size:'40sp'
			background_color: 1,1,0,0.7
		    on_press: app.root.current = 'Mood'
		Button:
			size_hint:1,0.2
			text: 'Depression'
			font_size:'40sp'
			background_color: 1,1,0,0.7
		    on_press: app.root.current = 'Depression'
		Button:
			size_hint:1,0.2
			text: 'Sleep'
			font_size:'40sp'
			background_color: 1,1,0,0.7
		    on_press: app.root.current = 'Sleep'

